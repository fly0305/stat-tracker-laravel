{
    "name": "pragmarx/tracker",

    "description": "A Laravel 4.1+ user activity tracker",

    "keywords": ["tracking", "logging", "user agent", "mobile detection", "laravel"],

    "license": "MIT",

    "authors": [
        {
            "name": "Antonio Carlos Ribeiro",
            "email": "acr@antoniocarlosribeiro.com",
            "role": "Creator & Designer"
        }
    ],

    "require": {
        "php": ">=5.3.7",
        "illuminate/foundation": "4.*",
        "illuminate/support": "4.*",
        "rhumsaa/uuid": "2.6.*@dev",
        "mobiledetect/mobiledetectlib": "dev-master",
        "tobie/ua-parser" : "dev-master"
    },

    "require-dev": {
        "mockery/mockery": "0.8.0"
    },

    "autoload": {
        "psr-4": {
            "PragmaRX\\Tracker\\": "src/"
        }
    },

    "extra": {
            "component": "package",
            "frameworks": ["Laravel 4"],
            "versions": {
                    "0.1.0": "master"
            },
            "branch-alias": {
                    "dev-master": "0.1.0-dev"
            }
    },

    "minimum-stability": "dev"
}


